<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no">
    <meta charset="utf-8">
    <title>Bicycling layer</title>
    <style>
      html, body, #map-canvas {
        height: 100%;
        margin: 0px;
        padding: 0px
      }
    </style>
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&signed_in=true"></script>
    <script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
    <script>
      var dataPoints = {};
      var mapData;
      var map;

      $.getJSON("/data.json", function(locations){
        function initialize() {
          var myLatlng = new google.maps.LatLng(40.712784, -74.005941);

          var mapOptions = {
            zoom: 14,
            center: myLatlng
          };

          map = new google.maps.Map(
              document.getElementById('map-canvas'),
              mapOptions);

          mapData = locations.data.map(function(pt){
            var coordinates = pt.geometry.coordinates;
            var point = new google.maps.LatLng(coordinates[0], coordinates[1])
            var populationOptions = {
              strokeColor: '#FF0000',
              strokeOpacity: 0.8,
              strokeWeight: 2,
              fillColor: '#FF0000',
              fillOpacity: 0.35,
              map: map,
              center: point,
              radius: 100
            };

            new google.maps.Circle(populationOptions);
          })

          var bikeLayer = new google.maps.BicyclingLayer();
          bikeLayer.setMap(map);
        }

        initialize();
        // google.maps.event.addDomListener(window, 'load', initialize);
      })

    </script>
  </head>
  <body>
    <div id="map-canvas"></div>
  </body>
</html>